<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.dao.OrderMapper">
    <insert id="addOrder" parameterType="com.entity.Order">
        INSERT into easybuy_order(loginName,userAddress,createTime,cost,serialNumber)
        VALUES(#{loginName},#{userAddress},now(),#{cost},#{serialNumber})
    </insert>
    <select id="findByid" resultType="java.lang.Integer" parameterType="java.lang.String">
        select id from easybuy_order where serialNumber=#{serialNumber}
    </select>
</mapper>