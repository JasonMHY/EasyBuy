<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.dao.OrderDetailMapper">
    <insert id="addOrderDetail" parameterType="com.entity.OrderDetail">
        insert into easybuy_order_detail(orderId,productId,quantity,cost)
        values(#{orderId},#{productId},#{quantity},#{cost})
    </insert>
</mapper>